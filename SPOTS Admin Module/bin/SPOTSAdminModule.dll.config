<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="Audit" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="Logs\%date{yyyy}\%date{MM}\%date{dd}\%SPOTS_AUDIT_%property{log4net:HostName}_%date{ddMMyyyy_HH}.LOG" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value="yyyyMMdd'.txt'" />
      <maxSizeRollBackups value="500" />
      <maximumFileSize value="5MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%d{ddMMyyyy HH:mm:ss}]%message%newline" />
      </layout>
    </appender>
    <logger additivity="false" name="AuditLogger">
      <level value="ALL" />
      <appender-ref ref="Audit" />
    </logger>
    <logger additivity="false" name="PerformanceLogger">
      <level value="ALL" />
      <appender-ref ref="Performance" />
    </logger>
  </log4net>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
